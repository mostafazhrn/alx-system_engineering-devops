#!/usr/bin/env bash
#This script shall displays num from 1 to 20 with prnt bd lck

x=1
while [ $x -le 20 ]
do
  case $x in
    4)
      echo "$x bad luck from china"
      ;;
    9)
      echo "$x bad luck from japan"
      ;;
    17)
      echo "$x bad luck from italy"
      ;;	
    *)
      echo "$x"
      ;;
    esac
    ((x++))
done
